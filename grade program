#include<stdio.h>
#include<conio.h>
main()
{
	printf("Enter your grade is :");
	char grade ='C';
	scanf("%c", &grade);
	
	switch(grade)
	{
		case'A':
			printf("Wow!\n");
			break;
		case'B':
			printf("Well done\n");
			break;
		case'C':
			printf("Awesome\n");
			break;
		case'D':
			printf("Better\n");
			break;
		case'F':
			printf("Better luck, next time\n");
			break;
		default:
			printf("Grade is invalid\n");
			
	}
}
